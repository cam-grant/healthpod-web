  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="header">

  </div>
  <div id="main">
    <%= yield %>
    <%#= render :partial => 'partials/user_data' %>
