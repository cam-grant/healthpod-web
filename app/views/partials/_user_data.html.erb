<% if @user_data %>
<style>
table.dump tr td {
  border-bottom: solid 1px #ccc;
  padding: 5px 10px;
}
</style>
<table class="dump">
  <% for attribute in @user_data.attributes.keys %>
    <tr>
      <td><%= attribute %></td>
      <td><%= @user_data.attributes[attribute].to_s %></td>
    </tr>
  <% end %>
  <tr>
    <td>Created at (precise)</td>
    <td><%= @user_data.created_at.strftime('%Y-%m-%d %H:%M:%S.%N') %></td>
  </tr>
  <tr>
    <td>Updated at (precise)</td>
    <td><%= @user_data.updated_at.strftime('%Y-%m-%d %H:%M:%S.%N') %></td>
  </tr>
</table>
<% end %>
